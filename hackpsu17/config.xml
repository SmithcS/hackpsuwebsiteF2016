<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.hackpsu" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>HackPSU</name>
    <description>
        HackPSU website/app
    </description>
    <author email="sam6258@psu.edu">
        Scott Miller
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <plugin name="cordova-plugin-cocoapod-support" spec="~1.2.14" />
    <plugin name="phonegap-plugin-push" spec="1.6.0">
    	<param name="SENDER_ID" value="838368032544" />
    </plugin>
</widget>
